   "source": [
    "def log_decorator(func):\n",
    "\n",
    "    def inner_func():\n",
    "        print('Before executing function')\n",
    "        func()\n",
    "        print('After executing function')\n",
    "\n",
    "    return inner_func"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "@log_decorator\n",
    "def my_func():\n",
    "    print('Inside my func')"
   ]
